    "electron-start": "cross-env NODE_ENV=production && electron .",
    "electron-dev": "webpack-dev-server & nodemon --watch . --exec electron .",
    "dev-fullstack": "concurrently --kill-others --names \"webpack,electron\" \"npm run dev\" \"npm run electron-dev\""
