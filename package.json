    "server": "cross-env NODE_ENV=development nodemon server/server.ts",
    "proxy": "cross-env NODE_ENV=development nodemon server/proxy.ts",
    "client": "npx webpack serve --mode development --progress --hot --color --stats-error-details --stats-children",
    "dev": "concurrently \"webpack-dev-server\" \"NODE_ENV=production nodemon server/server.ts\"",
    "proxy-prod": "cross-env NODE_ENV=development nodemon server/proxy.ts",
    "server-prod": "cross-env NODE_ENV=production nodemon server/server.ts",
    "client-prod": "npx webpack serve --mode production --progress --hot --color",
